{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": ".",
    "types": [],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "html-react-parser": ["."],
      "html-react-parser/lib/*": ["./lib/*"]
    },
    "jsx": "react"
  },
  "files": ["index.d.ts", "lib/dom-to-react.d.ts"],
  "include": ["test/types/**/*.ts", "test/types/**/*.tsx"]
}
