{
  "env": {
    "browser": true,
    "commonjs": true,
    "jest": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-debugger": "error",
    "no-console": "error",
    "prettier/prettier": "error"
  }
}
